{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "description": "riqra api",
  "repository": {
    "type": "git",
    "url": "https://github.com/riqra/api"
  },
  "author": "Riqra",
  "scripts": {
    "build": "swc . --include-dotfiles --config-file .swcrc --out-dir .dist --copy-files --ignore 'node_modules' --ignore '**/__tests__/**' --only 'src/**/*' --only '.sequelizerc' --extensions '.ts' --extensions '.js'",
    "dev": "nodemon src",
    "start": "node .dist/src",
    "lint": "eslint --max-warnings 0 .",
    "typecheck": "tsc",
    "db:prepare": "cross-env TZ=utc ./scripts/db-prepare.sh",
    "db:fresh": "cross-env TZ=utc npm run db:prepare -- -t",
    "test": "cross-env TZ=utc NODE_OPTIONS='--max-old-space-size=4096' node --expose-gc ./node_modules/jest/bin/jest.js -i --logHeapUsage --colors --forceExit",
    "heroku-cleanup": "npm prune --production",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix --max-warnings 0"
    ]
  },
  "nodemonConfig": {
    "env": {
      "TZ": "utc",
      "NODE_ENV": "development",
      "DEBUG": "api:*",
      "DEBUG_COLORS": true,
      "DEBUG_DEPTH": "null",
      "DEBUG_SHOW_HIDDEN": "false"
    },
    "exec": "node -r @swc-node/register",
    "ext": "ts,js,json",
    "ignore": [
      "**/__tests__/**/**.{ts,js}",
      "**/seeders/**/**.{ts,js}"
    ]
  },
